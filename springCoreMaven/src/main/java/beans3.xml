<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="iphone6s" class="com.clghks.springcore.SmartPhone">
		<property name="name" value="아이폰6s" />
		<property name="price" value="1090000" />
	</bean>
	
	<bean id="iphone6" class="com.clghks.springcore.SmartPhone">
		<property name="name" value="아이폰6" />
		<property name="price" value="990000" />
	</bean>
	
	<bean id="g4" class="com.clghks.springcore.SmartPhone">
		<property name="name" value="G4" />
		<property name="price" value="1000" />
	</bean>
	
	<bean id="g5" class="com.clghks.springcore.SmartPhone">
		<property name="name" value="G5" />
		<property name="price" value="11000" />
	</bean>
</beans>